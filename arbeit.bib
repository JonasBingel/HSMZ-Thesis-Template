@article{jansen-preilowski_arbeitszeitgestaltung_2020,
	title = {Arbeitszeitgestaltung in der digitalisierten {Arbeitswelt}: {Ein} systematisches {Literatur} {Review} zur {Wirkung} von {Arbeitszeitverkürzung} in {Bezug} auf die psychische {Gesundheit}},
