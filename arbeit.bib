@article{brandstotter_roboterbasierter_2023,
	title = {Roboterbasierter {3D}-{Druck} mit neuen {Dimensionen}},
	volume = {140},
	issn = {0932-383X, 1613-7620},
	url = {https://link.springer.com/10.1007/s00502-023-01163-z},
	doi = {10.1007/s00502-023-01163-z},
	abstract = {Zusammenfassung
            Der roboterbasierte 3D-Druck ermöglicht neue Dimensionen der additiven Fertigung. Im Gegensatz zu herkömmlichen 3D-Druckern, die in der Regel auf einem festen Druckbett arbeiten, wird ein Roboterarm als Bewegungsplattform verwendet. Dies eröffnet neue Möglichkeiten in Bezug auf Designfreiheit, Material- und Prozessvielfalt. Da Roboterarme komplexe Bewegungen ausführen können, sind sie in der Lage, 3D-Objekte mit sehr komplexen Strukturen und Geometrien zu drucken, die Notwendigkeit von Stützmaterial zu reduzieren oder zu eliminieren sowie eine Vielzahl von Materialien zu verarbeiten, darunter auch solche mit sehr unterschiedlichen Eigenschaften, wie Elastomere, Metalle oder Verbundwerkstoffe. Das Druckverfahren kann je nach Material und Anforderungen angepasst werden. Dies eröffnet die Möglichkeit, ein breites Spektrum von Anwendungen zu optimieren, insbesondere im Hinblick auf Design, Funktionsintegrität und Prozessoptimierung.
          , 
            Abstract
            Robot-based 3D printing enables new dimensions in additive manufacturing. In contrast to conventional 3D printers, which usually work on a fixed print bed, a robotic arm is used as a motion platform. This opens up new possibilities in terms of design freedom, material and process diversity. Because robotic arms can perform complex movements, they are able to print 3D objects with very complex structures and geometries, reduce or eliminate the need for support material and process a variety of materials, including those with very different properties, such as elastomers, metals or composites. The printing process can be adapted depending on the material and requirements. This opens up the possibility of optimizing a wide range of applications, especially in terms of design, functional integrity and process optimization.},
	language = {de},
	number = {6},
	urldate = {2024-03-16},
	journal = {e \& i Elektrotechnik und Informationstechnik},
	author = {Brandstötter, Mathias and Petersmann, Sandra and Bosch, Julian},
	month = oct,
	year = {2023},
	pages = {536--540},
	file = {Brandstötter et al. - 2023 - Roboterbasierter 3D-Druck mit neuen Dimensionen.pdf:C\:\\Users\\Patri\\Zotero\\storage\\JBA734BF\\Brandstötter et al. - 2023 - Roboterbasierter 3D-Druck mit neuen Dimensionen.pdf:application/pdf},
}

